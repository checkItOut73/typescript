{
  "name": "typescript-skeleton",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build:browser": "webpack --config build/configs/webpack.config.js",
    "build:server": "tsc",
    "watch:server": "npm run build:server -- --watch",
    "format": "prettier src --write \"{./**/*.{json,js,jsx,ts,tsx},.prettierrc}\"",
    "start": "node ./dist/server/index.js",
    "start:dev": "nodemon ./dist/server/index.js"
  },
  "_moduleAliases": {
    "@src": "dist",
    "@browser": "dist/browser",
    "@server": "dist/server",
    "@configs": "build/configs"
  },
  "dependencies": {
    "fastify": "^3.14.2",
    "fastify-static": "^4.0.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "fastify-express": "^0.3.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "merge-options": "^3.0.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^4.1.0",
    "ws": "^7.4.4"
  }
}
