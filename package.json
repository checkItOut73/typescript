{
  "name": "typescript-skeleton",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build:browser": "webpack --config build/configs/webpack.config.js",
    "build:server": "tsc",
    "watch:server": "npm run build:server -- --watch",
    "format": "prettier src --write \"{./**/*.{json,js,jsx,ts,tsx},.prettierrc}\"",
    "start": "node ./dist/server/index.js",
    "start:dev": "nodemon ./dist/server/index.js"
  },
  "_moduleAliases": {
    "@src": "dist",
    "@browser": "dist/browser",
    "@server": "dist/server",
    "@configs": "build/configs"
  },
  "dependencies": {
    "fastify": "^3.22.1",
    "fastify-static": "^4.4.2",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "fastify-express": "^0.3.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "merge-options": "^3.0.4",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "ws": "^8.2.3"
  }
}
